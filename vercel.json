
{
    "version": 2,
    "builds": [
        {
            "src": "vercel-django-handler.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel-django-handler.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "saveplate.settings" 
    }
}